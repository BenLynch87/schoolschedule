<?xml version='1.0' encoding='utf-8'?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lynch.schoolschedule">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SchoolSchedule">

        <activity android:name=".UI.AssessmentActivity" />
        <activity android:name=".UI.AssessmentDetailActivity" />
        <activity android:name=".UI.AssessmentReportActivity" />
        <activity android:name=".UI.ClassActivity" />
        <activity android:name=".UI.ClassDetailActivity" />
        <activity android:name=".UI.LoginRegisterActivity" />
        <activity android:name=".UI.LogoutActivity" />
        <activity android:name=".UI.ClassSelectActivity" />
         <activity android:name=".UI.MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.OptionsActivity" />
        <activity android:name=".UI.TermDetailActivity" />
        <activity android:name=".UI.TermsActivity" />

    </application>
</manifest>
